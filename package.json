{
  "name": "obsidian-link-mention",
  "version": "1.0.0",
  "description": "Render empty markdown links as rich inline pills with favicon and page title",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "oxfmt --check src/ && oxlint src/ && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "format": "oxfmt --write src/",
    "format:check": "oxfmt --check src/",
    "lint": "oxlint src/",
    "test": "vitest run",
    "test-titles": "npx tsx src/test-titles.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "devDependencies": {
    "@codemirror/language": "^6.12.2",
    "@codemirror/state": "^6.5.4",
    "@codemirror/view": "^6.39.15",
    "@types/node": "^16.11.6",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "jsdom": "^28.1.0",
    "obsidian": "latest",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vitest": "^4.0.18"
  }
}
